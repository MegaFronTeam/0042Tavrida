mixin sParntentsRows(data)
	// start sParntentsRows
	+b.SECTION.sParntentsRows.section#sParntentsRows&attributes(attributes)
		.container
			+b.section-title
				h2=data.title
			block
	// end sParntentsRows

mixin sParntentsRow(title)
	+e.H4.sub-title=title
	+e.row.row
		block


mixin sPRowItem(imgNum, txt)
	.col-6.col-lg-4.col-xl-3
		+e.img
			img(loading="lazy" src=`img/@2x/pr-logo-${imgNum}.png` alt="")
		+e.name.text-center=txt