extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug
include ../blocks/sAbout/_sAbout.pug
include ../blocks/sVantage/_sVantage.pug
include ../blocks/sProjects/_sProjects.pug
include ../blocks/sResidents/_sResidents.pug
include ../blocks/sAchivment/_sAchivment.pug
include ../blocks/sStory/_sStory.pug
include ../blocks/sFaumos/_sFaumos.pug
include ../blocks/sNews/_sNews.pug
include ../blocks/sPartners/_sPartners.pug
include ../blocks/sFestival/_sFestival.pug

block variables
	- let  mainPage= null
block title
	- let  title= 'title'
	title= title
block pagecontent
	+mMenu()
	+header()
		+top-nav()
	+headerBlock({
		title: `Арт-кластер <br> «Таврида»`,
		video: `https://vk.com/video_ext.php?oid=-89840945&id=456240410&hash=fece856eef05f5ea&hd=4`
	})
	+sAbout({
		bg: 'about-bg-1-new.jpg',
		title: 'Об арт-кластере',
		text: `Арт-кластер &laquo;Таврида&raquo; ‒ это экосистема проектов для реализации творческого и&nbsp;профессионального потенциала молодых деятелей культуры и&nbsp;искусств`
	})
	+sVantage()
	+sProjects()
	+sFestival()
	+sAchivment()
	+sResidents()
	+sStory()
	+sFaumos()
	+sNews()
	+sForm()
	+sPartners()
	+footer()